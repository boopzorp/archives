{
  "manifest_version": 3,
  "name": "archives",
  "version": "1.0",
  "description": "Save links to your archives account.",
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "host_permissions": [
    "https://arch1ves.vercel.app/*"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
